{
  "name": "expo-permissions-store",
  "version": "0.1.1",
  "description": "Redux Toolkit (RTK Query) store for managing Expo permissions",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && tsc --project tsconfig.build.json",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "spell": "cspell \"**/*\"",
    "knip": "knip",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "release-it"
  },
  "keywords": [
    "expo",
    "permissions",
    "redux",
    "rtk-query",
    "react-native"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tupe12334/expo-permissions-store.git"
  },
  "peerDependencies": {
    "@reduxjs/toolkit": ">=1.9.0",
    "react": ">=18.0.0",
    "react-redux": ">=8.0.0"
  },
  "peerDependenciesMeta": {
    "expo-camera": {
      "optional": true
    },
    "expo-media-library": {
      "optional": true
    },
    "expo-location": {
      "optional": true
    },
    "expo-notifications": {
      "optional": true
    },
    "expo-contacts": {
      "optional": true
    },
    "expo-calendar": {
      "optional": true
    },
    "expo-tracking-transparency": {
      "optional": true
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@eslint/js": "^9.0.0",
    "@reduxjs/toolkit": "^2.0.0",
    "@release-it/conventional-changelog": "^10.0.0",
    "@types/react": "^18.2.0",
    "@vitest/coverage-v8": "^4.0.18",
    "cspell": "^9.6.4",
    "eslint": "^9.0.0",
    "eslint-config-agent": "^1.9.3",
    "eslint-config-prettier": "^9.0.0",
    "expo-calendar": "^14.1.4",
    "expo-camera": "^16.1.11",
    "expo-contacts": "^14.2.6",
    "expo-location": "^18.1.6",
    "expo-media-library": "^17.1.7",
    "expo-notifications": "^0.29.14",
    "expo-tracking-transparency": "^5.2.4",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "knip": "^5.0.0",
    "lint-staged": "^15.0.0",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-native": "^0.76.0",
    "react-redux": "^9.0.0",
    "release-it": "^19.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.28.0"
}
